# }

function  curlc() {
    echo "${1}"
    if [[ -z ${2} ]]; then
        curl -O -C - -k -L $1
    else
        echo "$2"
        curl -C - -k -L --create-dirs $1 -o $2
    fi
}

function jgc() {
    pname=""
    if [[ -z ${1} ]]; then
        pname="RemoteMavenServer"
    else
        pname="${1}"
    fi
    echo "${pname}"
    jcmd $(jps -l | grep $pname | awk -F ' ' '{print $1}') GC.run
}

