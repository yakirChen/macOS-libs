{
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 11,
    "editor.fontFamily": "Hack", //, Menlo, Monaco, 'Courier New', monospace",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.colorTheme": "Default High Contrast Light",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.maxTokenizationLineLength": 99999999,
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    // https://rust-analyzer.github.io/manual.html
    "rust-analyzer.server.extraEnv": {
        "RUSTUP_TOOLCHAIN": "nightly"
    },
    "rust-analyzer.checkOnSave.command": "check",
    "rust-analyzer.server.path": "/Users/yakir/local/rust/toolchains/nightly-x86_64-apple-darwin/bin/rust-analyzer",
    "rust-analyzer.semanticHighlighting.strings.enable": true,
    "rust-analyzer.highlightRelated.breakPoints.enable": true,
    "rust-analyzer.highlightRelated.references.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.enable": true,
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.lru.capacity": 1000,
    "json.maxItemsComputed": 5000000,
    "editor.foldingMaximumRegions": 65000, // 分析器最大缓存深度
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "debug.console.fontSize": 11,
    "sourcekit-lsp.serverPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
    "sourcekit-lsp.trace.server": "messages",
    "editor.accessibilitySupport": "on",
    "audioCues.lineHasBreakpoint": "off",
    "audioCues.lineHasError": "off",
    "audioCues.lineHasFoldedArea": "off",
    "audioCues.lineHasInlineSuggestion": "off",
    "audioCues.noInlayHints": "off",
    "audioCues.onDebugBreak": "off",
    "editor.largeFileOptimizations": false,
    "explorer.confirmDelete": false,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.workspaceParsingPriority": "high",
    "C_Cpp.intelliSenseEngine": "Disabled",
    // 修改.build位置
    // "sde.swiftBuildingParams": [
    //     "build",
    //     "--build-path",
    //     "../swift-build"
    //   ]
}