{
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 11,
    "editor.fontFamily": "Hack",
    "workbench.colorTheme": "Default Light Modern",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.maxTokenizationLineLength": 1000000000000000000,
    "workbench.startupEditor": "none",
    "security.workspace.trust.untrustedFiles": "open",
    // https://rust-analyzer.github.io/manual.html
    "rust-analyzer.server.extraEnv": {
        "RUSTUP_TOOLCHAIN": "stable"
    },
    "rust-analyzer.server.path": "/Users/yakir/local/rust/toolchains/stable-aarch64-apple-darwin/bin/rust-analyzer",
    "rust-analyzer.semanticHighlighting.strings.enable": true,
    "rust-analyzer.highlightRelated.breakPoints.enable": true,
    "rust-analyzer.highlightRelated.references.enable": true,
    "rust-analyzer.inlayHints.closingBraceHints.enable": true,
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.lru.capacity": 1000,
    "json.maxItemsComputed": 5000000,
    "editor.foldingMaximumRegions": 65000, // 分析器最大缓存深度
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "debug.console.fontSize": 11,
    // "swift.sourcekit-lsp.serverPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
    "swift.sourcekit-lsp.trace.server": "messages",
    "editor.accessibilitySupport": "off",
    "editor.largeFileOptimizations": false,
    "explorer.confirmDelete": false,
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.workspaceParsingPriority": "high",
    "C_Cpp.intelliSenseEngine": "disabled",
    "FSharp.dotnetRoot": "/Volumes/sm/servers/dotnet",
    "window.commandCenter": true,
    "clangd.path": "/usr/bin/clangd",
    // 修改.build位置
    // "sde.swiftBuildingParams": [
    //     "build",
    //     "--build-path",
    //     "../swift-build"
    //   ]
    "makefile.makeDirectory": "clang",
    "python.defaultInterpreterPath": "/Users/yakir/local/brew/bin/python3",
    "terminal.integrated.fontSize": 11,
    "git.openRepositoryInParentFolders": "never",
    "[python]": {
        "editor.formatOnType": true
    },
    "editor.minimap.enabled": false,
    "settingsSync.ignoredSettings": [
        "-clangd.path"
    ],
    "msg.shell.enforceHttps": false,
    "msg.editor.wordWrapColumn": 220,
    "msg.dbEditor.defaultEditor": "script",
    "msg.shellSession.startLanguage": "sql",
    "workbench.editor.empty.hint": "hidden",
    "[ruby]": {
        "editor.defaultFormatter": "Shopify.ruby-lsp",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.trimFinalNewlines": true,
        "editor.rulers": [
            120
        ],
        "editor.semanticHighlighting.enabled": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.editorAssociations": {
        "*.plist": "default"
    },
    "update.showReleaseNotes": false,
    "accessibility.signals.lineHasError": {
        "sound": "off"
    },
    "accessibility.signals.lineHasFoldedArea": {
        "sound": "off"
    },
    "accessibility.signals.lineHasBreakpoint": {
        "sound": "off"
    },
    "accessibility.signals.lineHasInlineSuggestion": {
        "sound": "off"
    },
    "accessibility.signals.onDebugBreak": {
        "sound": "off"
    },
    "accessibility.signals.noInlayHints": {
        "sound": "off"
    },
    "makefile.configureOnOpen": true,
    "window.titleBarStyle": "custom",
    "rubyLsp.enableExperimentalFeatures": true,
    // https://marketplace.visualstudio.com/items?itemName=be5invis.vscode-custom-css
    // "vscode_custom_css.imports": [
    //     "file:///Users/yakir/Library/Mobile Documents/com~apple~CloudDocs/Developer/macOS-libs/vscode/vscode.css"
    // ],
