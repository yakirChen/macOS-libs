function myip(){
    curl -L -s https://myipv4.p1.opendns.com/get_my_ip | jq
}

function reset_git() {
    rm -rf
    branch=$(git status | grep "On branch" | awk '/On branch/ {print $3}')
    echo "branch is ${branch}"
    if [[ ! "main" -eq "${branch}" && ! "master" -eq ${branch} && ! "develop" -eq "${branch}" ]]; then
        echo "not support branch"
        return -1
    fi
    git reset . && \
    git checkout -- . && \
    git checkout "origin/${branch}" &&  \
    git branch -D "${branch}" ; \
    git status && \
    git checkout "${branch}" && \
    git status
}

function prune_remote() {
    git remote prune origin
}

function yl() {
    echo "$1"
    ${OPT}/bin/yt-dlp_macos --proxy socks5://127.0.0.1:7890 -v --sub-langs "zh.*,en*" --write-auto-subs --write-subs $1
}

function ylc() {
    echo "$1"
    ${OPT}/bin/yt-dlp_macos --proxy socks5://127.0.0.1:7890 -v --sub-langs "zh.*,en*" --cookies-from-browser chrome --write-auto-subs --write-subs $1
}

